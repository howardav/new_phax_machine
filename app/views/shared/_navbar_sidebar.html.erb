  <nav class="navbar navbar-expand-lg navbar-dark bg-dark navbar-fixed-top">
  	<ul class="navbar-nav mr-auto">
  		<li class="nav-item">
        <%= link_to "Home", root_path, class: "nav-link" %>
      </li>
      <% if logged_in? %>
      	<% if is_admin? %>
	      	<li class="nav-item">
	      		<%= link_to "Manage Clients", clients_path, class: "nav-link" %>
		      </li>
		      <li class="nav-item">
	      		<%= link_to "Manage Fax Numbers", fax_numbers_path, class: "nav-link" %>
		      </li>
      	<% end %>
      	<li class="nav-item">
      		<%= link_to "My Profile", user_path(current_user.id), class: "nav-link" %>
	      </li>
