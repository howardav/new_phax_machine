<%= form_for email do |f| %>
	<%= f.hidden_field :fax_number_id, value: "#{@fax_number.id}" %>
	<%= f.hidden_field :client_id, value: "#{@fax_number.client.id}" %>
	<div class="form-group">
		<%= f.label :email, "Email Address" %>
		<%= f.email_field :email, class: "form-control"%>
	</div>
	<div class="form-group">
		<%= f.label :fax_number, "Caller ID Number" %>
		<%= f.phone_field :fax_number, value: "#{@fax_number.fax_number}", class: "form-control" %>
	</div>
	<div class="form-group">
		<%= f.submit "Submit", class: "btn btn-primary" %>
	</div>
<% end %>